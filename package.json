{
  "name": "valtimo-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --port 4200 --proxy-config conf/proxy.conf.json",
    "build-all": "npm run build-libs && npm run build",
    "watch-all": "run-s watch-all:*",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "build-libs": "run-s build:*",
    "build:shared": "ng build @valtimo-portal/shared",
    "build:authentication": "ng build @valtimo-portal/authentication",
    "build:graphql": "ng build @valtimo-portal/graphql",
    "build:form": "ng build @valtimo-portal/form",
    "build:case": "ng build @valtimo-portal/case",
    "build:nl-material": "ng build @valtimo-portal/nl-material",
    "serve:app": "wait-on dist/valtimo-portal/nl-material/public-api.d.ts && npm run start",
    "watch-all:clean": "npm run clean",
    "watch-all:serve": "run-p watch:* serve:app",
    "watch:shared": "ng build @valtimo-portal/shared --watch",
    "watch:authentication": "wait-on dist/valtimo-portal/shared/public-api.d.ts && ng build @valtimo-portal/authentication --watch",
    "watch:graphql": "wait-on dist/valtimo-portal/authentication/public-api.d.ts && ng build @valtimo-portal/graphql --watch",
    "watch:form": "wait-on dist/valtimo-portal/graphql/public-api.d.ts && ng build @valtimo-portal/form --watch",
    "watch:case": "wait-on dist/valtimo-portal/form/public-api.d.ts && ng build @valtimo-portal/case --watch",
    "watch:nl-material": "wait-on dist/valtimo-portal/case/public-api.d.ts && ng build @valtimo-portal/nl-material --watch",
    "test:app": "jest --watchAll --modulePathIgnorePatterns=projects",
    "test:shared": "jest --watchAll --testPathPattern=projects/valtimo-portal/shared",
    "test:authentication": "jest --watchAll --testPathPattern=projects/valtimo-portal/authentication",
    "test:graphql": "jest --watchAll --testPathPattern=projects/valtimo-portal/graphql",
    "test:form": "jest --watchAll --testPathPattern=projects/valtimo-portal/form",
    "test:case": "jest --watchAll --testPathPattern=projects/valtimo-portal/case",
    "test:nl-material": "jest --watchAll --testPathPattern=projects/valtimo-portal/nl-material",
    "graphql:schema": "apollo client:download-schema ./projects/valtimo-portal/graphql/src/lib/schemas/portal-api-schema.json",
    "graphql:generate": "run-s graphql:generate:*",
    "graphql:generate:codegen": "graphql-codegen",
    "graphql:generate:fix-imports": "node ./scripts/fix-graphql-types-imports.js",
    "build:test": "npm run build-libs && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --prod --configuration=test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.7",
    "@angular/cdk": "^11.2.3",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/elements": "^11.2.7",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.7",
    "@angular/material": "^11.2.3",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@apollo/client": "^3.3.12",
    "@briebug/jest-schematic": "^3.1.0",
    "@formio/angular": "^5.0.3-rc.2",
    "@formio/angular-material": "^2.0.0",
    "@gilsdav/ngx-translate-router": "^3.1.6",
    "@ngx-translate/core": "^13.0.0",
    "apollo-angular": "^2.4.0",
    "graphql": "^15.5.0",
    "hammerjs": "^2.0.8",
    "keycloak-angular": "^8.1.0",
    "keycloak-js": "^12.0.4",
    "material-design-icons": "^3.0.1",
    "ngx-translate-multi-http-loader": "^3.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@graphql-cli/codegen": "1.17.22",
    "@graphql-cli/coverage": "2.1.0",
    "@graphql-cli/validate": "2.1.0",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-apollo-angular": "^2.3.2",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/jest": "26.0.22",
    "@types/node": "^12.11.1",
    "apollo": "^2.32.5",
    "codelyzer": "^6.0.0",
    "formiojs": "^4.13.0-rc.20",
    "graphql-cli": "^4.1.0",
    "jest": "26.6.3",
    "ng-packagr": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "replace-in-files": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2",
    "wait-on": "^5.3.0"
  }
}
